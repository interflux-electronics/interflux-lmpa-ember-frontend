{{!-- https://github.com/ronco/ember-cli-head --}}
<title>{{model.title}}</title>
<meta
  name="description"
  content={{model.description}}
>
<link
  rel="canonical"
  href={{model.canonical}}
>
<meta
  name="robots"
  content="index, follow"
>

{{!-- https://developers.facebook.com/docs/sharing/webmasters/ --}}
<meta
  property="og:type"
  content="website"
>
<meta
  property="og:url"
  content={{model.canonical}}
>
<meta
  property="og:title"
  content={{model.title}}
>
<meta
  property="og:description"
  content={{model.description}}
>

{{!-- https://developers.facebook.com/docs/sharing/best-practices#images --}}
{{!-- <meta property="og:image" content="https://xxx@1200x630-fingerprinted.jpg">
<meta property="og:image:secure_url" content="https://xxx@1200x630-fingerprinted.jpg">
<meta property="og:image:type" content="image/jpeg">
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="630"> --}}

{{!--
Browser search theme colours
For Chrome, Firefox OS and Opera
https://developers.google.com/web/fundamentals/design-and-ux/browser-customization/
